/// url = "http://termx.health/fhir/StructureMap/code2Identifier"

uses "http://hl7.org/fhir/StructureDefinition/string" alias string as source
uses "http://hl7.org/fhir/StructureDefinition/Identifier" alias Identifier as target

group example(source string : string, target Identifier : Identifier) {
  string -> Identifier as a then {
      string.value as b -> a.value = b "dset_13";
      string -> a.system = ('https://termx.org/sid/pid/est/ni') as constant4 "const_14";
  } "rule_11";
}

